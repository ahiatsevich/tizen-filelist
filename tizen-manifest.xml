<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<manifest xmlns="http://tizen.org/ns/packages" api-version="2.3.1" package="org.ahiatsevich.filelist" version="0.0.1">
    <author email="aliaksei.hiatsevich@gmail.com">Aliaksei Hiatsevich</author>
    <description>Simple File file application</description>
    <profile name="wearable"/>
    <ui-application appid="org.ahiatsevich.filelist" exec="filelist" multiple="false" nodisplay="false" taskmanage="true" type="capp">
        <label>filelist</label>
        <icon>filelist.png</icon>
    </ui-application>
    <privileges>
        <privilege>http://tizen.org/privilege/mediastorage</privilege>
        <privilege>http://tizen.org/privilege/externalstorage</privilege>
    </privileges>
    <feature name="http://tizen.org/feature/screen.size.normal.320.320">true</feature>
</manifest>
